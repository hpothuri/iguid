FROM XXSTG_CHILD HDR,XXSTG_PERSON PERS     
WHERE HDR.PERSON_ID = PERS.PERSON_ID     
and nvl((select floor(months_between(TRUNC(:bindInvDate),   
  to_date((select DATE_OF_BIRTH from XXSTG_PERSON where person_id = HDR.CONTACT_PERSON_ID), 'DD-MM-YYYY'))/12) as age from dual),0)>=4    
and nvl((select floor(months_between(TRUNC(:bindInvDate),   
  to_date((select DATE_OF_BIRTH from XXSTG_PERSON where person_id = HDR.CONTACT_PERSON_ID), 'DD-MM-YYYY'))/12) as age from dual),0)<=19]]></SQLQuery>
