<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CheckEmployeeReqExistsROVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Variable
    Name="bind_reqtype"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bind_empid"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <SQLQuery><![CDATA[select count(*) reccount from XXHCM_OVERTIME_HEADERS_ALL  where EMP_ID = :bind_empid and request_number like '%'||:bind_reqtype||'%' and REQUEST_DATE > add_months(sysdate,-6)]]></SQLQuery>
  <ViewAttribute
    Name="Reccount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="BIGINT"
    AliasName="RECCOUNT"
    Expression="RECCOUNT"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="Reccount"/>
  </AttrArray>
</ViewObject>
